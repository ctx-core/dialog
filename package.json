{
	"name": "@ctx-core/dialog",
	"version": "12.0.25",
	"description": "ctx-core dialog",
	"keywords": [
		"ctx-core",
		"dialog"
	],
	"homepage": "https://github.com/ctx-core/dialog#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/dialog/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/dialog/issues"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"scripts": {
		"build": "npm run clean && npm run generate_ctx_I_file && npm run compile && npm run symlink",
		"clean": "rm -f tsconfig.tsbuildinfo && rm -f dist/**/*.js && rm -f dist/**/*.d.ts && rm -f dist/**/*.map",
		"compile": "./node_modules/.bin/tsc -b .",
		"symlink": "./node_modules/.bin/symlink_src_assets_to_dist .",
		"exec": "$@",
		"prepublishOnly": "npm run compile",
		"generate_ctx_I_file": "./node_modules/.bin/generate_ctx_I_file"
	},
	"dependencies": {
		"@ctx-core/array": "workspace:^21.2.8",
		"@ctx-core/function": "workspace:^18.1.3",
		"@ctx-core/object": "workspace:^19.1.3",
		"@ctx-core/store": "workspace:^25.1.14",
		"svelte": "3.38.2"
	},
	"devDependencies": {
		"@ctx-core/ctx-core-package-tools": "workspace:^7.0.16",
		"@ctx-core/dev-tools": "workspace:^5.1.4",
		"typescript": "^4.3.2"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	}
}
